{
  "name": "demo-maps-api",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "npx cross-env DEBUG=demo-maps-api:* nodemon ./bin/www",
    "eslint": "eslint .",
    "prettier": "prettier --check ."
